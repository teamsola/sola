<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="member.bean.MemberDTO" alias="memberDTO"></typeAlias>
		<typeAlias alias="placeDTO" type="tip.bean.PlaceDTO"/>
		<typeAlias alias="expressDTO" type="tip.bean.ExpressDTO"/>
		<typeAlias alias="ledgerDTO" type="ledger.bean.LedgerDTO"/>
		<!-- 가계부 -->
		<typeAlias alias="proinfoDTO" type="proinfo.bean.ProinfoDTO"/>
		<!-- 전문지식 -->
	</typeAliases>
	<mappers>
		<mapper resource="mybatis/member-mapping.xml" />
		<mapper resource="mybatis/tip-mapping.xml"/>
		<mapper resource="mybatis/diary-mapping.xml"/>
        <mapper resource="mybatis/mall-mapping.xml"/>
        <mapper resource="mybatis/board-mapping.xml"/>
        <mapper resource="mybatis/ledger-mapping.xml"/>
		<!-- 가계부 -->
		<mapper resource="mybatis/proinfo-mapping.xml"/>
		<!-- 전문지식 -->
	</mappers>
</configuration>